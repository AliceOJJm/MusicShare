<% if(@songs) %>
    <p><strong></strong></p>
    <% @songs.each do |song| %>
        <p> <%= song.title %> </p>
        <p>  <%= song.performer %> </p>
        <%= audio_tag song.file.url.to_s, controls: true %>
        <p>  <%= link_to "Delete", song, method: :delete, confirm: "Are you sure you want to delete #{song.file}?" %> </p>
    <% end %>
<%else%>
    <p><strong>You didn't upload music yet.</strong></p>
<% end %>

<%= render "form" %>